<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root alignment="TOP_RIGHT" fillHeight="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="600.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/15" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" contentDisplay="CENTER" graphicTextGap="5.0" minHeight="-Infinity" minWidth="410.0" pickOnBounds="false" styleClass="textBox" stylesheets="@../css/main.css" text="Label" wrapText="true">
            <font>
                <Font name="Comic Sans MS" size="13.0" />
            </font>
            <HBox.margin>
                <Insets bottom="5.0" top="5.0" />
            </HBox.margin>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding></Label>
        <ImageView fx:id="displayPicture" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
            <HBox.margin>
                <Insets left="10.0" right="10.0" />
            </HBox.margin>
            <viewport>
                <Rectangle2D />
            </viewport></ImageView>
    </children>
    <padding>
        <Insets bottom="10.0" left="5.0" right="5.0" top="10.0" />
    </padding>
</fx:root>
